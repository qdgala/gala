<?xml version="1.0"?>
<openerp>
    <data noupdate="0">


        <record model="ir.ui.view" id="view_hd_product_import_demo_form">
            <field name="name">导入报价单</field>
            <field name="model">hd.product.import.demo</field>
            <field name="arch" type="xml">
                <form string="导入报价单">
                    <group>
                        <field name="excel"/>
                    </group>
                    <footer>
                        <button name="btn_import" string="导入产品" type="object" class="btn-primary"/>
                        <button name="btn_cb_import" string="导入成本价" type="object" class="btn-primary"/>
