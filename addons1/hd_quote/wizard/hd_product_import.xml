            <field name="view_type">form</field>
            <field name="domain"></field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_action_hd_product_import_demo" action="action_hd_product_import_demo" parent="base.menu_sale_config"
                  sequence="30" name="导入报价单"/>


    </data>
