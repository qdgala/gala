<?xml version="1.0"?>
<openerp>
    <data noupdate="0">

        <record model="ir.ui.view" id="view_oa_clxx_tree">
            <field name="name">车辆信息</field>
            <field name="model">oa.clxx</field>
            <field name="arch" type="xml">
                <tree string="车辆信息">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="jsr"/>
                    <field name="wlgs"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="view_oa_clxx_form">
            <field name="name">车辆信息</field>
            <field name="model">oa.clxx</field>
            <field name="arch" type="xml">
                <form string="车辆信息">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="code"/>
                            </group>
                            <group>
                                <field name="jsr"/>
                                <field name="wlgs"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="view_oa_clxx_search">
            <field name="name">车辆信息搜索视图</field>
            <field name="model">oa.clxx</field>
            <field name="arch" type="xml">
                <search string="车辆信息搜索视图">

                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_oa_clxx">
            <field name="name">车辆信息</field>
            <field name="res_model">oa.clxx</field>
            <field name="view_type">form</field>
            <field name="domain"></field>
            <field name="view_mode">tree,form</field>
            <!--<field name="context"></field>-->
            <field name="search_view_id" ref="view_oa_clxx_search"/>
            <field name="view_id" ref="view_oa_clxx_tree"/>
        </record>

        <menuitem id="menu_action_oa_clxx" action="action_oa_clxx" parent="stock.menu_stock_sched" sequence="130"
                  name="车辆信息"/>


    </data>
</openerp>